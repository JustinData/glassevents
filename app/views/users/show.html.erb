<h1><%= @user.first_name %> <%= @user.last_name %></h1>

<p>
	<%= @user.email %><br>
	<% if @user.admin == true %>
		You are an admin!
	<% end %>
</p>

<section class="admins">
	<h4><%= link_to "admins:", admins_path %></h4>
	<ul>
			<% users = User.all %>
			<% users.each do |user| %>
				<% if user[:admin] == true %>
		    		<li><%= user[:first_name] %> <% user[:last_name] %></li>

		    	<% end %>
			<% end %>	
		</ul>
</section>

<section class="services">
	<h4><%= link_to "services:", services_path %></h4>
	<% services = Service.all %>
	<ul>
		<% services.each do |s| %>
			<li><%= s.service_name %></li>
		<% end %>
	</ul>
</section>

